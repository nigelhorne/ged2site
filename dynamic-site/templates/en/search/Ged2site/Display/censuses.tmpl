<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
    "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
	<head>
		[% IF census %]
			<title>[% census.0.census %] entries in [% SiteTitle.Default.join(' ') %]</title>
		[% ELSE %]
			<title>Censuses for [% SiteTitle.Default.join(' ') %]</title>
		[% END %]
		<meta http-equiv="Content-Type" content="text/html; charset=windows-1252">
		<meta http-equiv="Content-Language" content="en">
		<meta name="robots" content="INDEX,FOLLOW">
		<meta name="language" content="ENGLISH">
		<meta name="object" content="Index">
		<meta name="owner" content="Nigel Horne">
		<meta name="Author" content="Nigel Horne">
		<meta name="Copyright" content="Copyright (C) 2025 Nigel Horne, all rights reserved">
		<!--
		<meta name="msvalidate.01" content="deadbeef" />
		-->
	</head>
	<body>
		<center>
			[% IF census %]
				<h1>[% census.0.census %] entries in [% SiteTitle.Default.join(' ') %]</h1>
			[% ELSE %]
				<h1>Censuses for [% SiteTitle.Default.join(' ') %]</h1>
			[% END %]
			[% IF error %]
				<p><font color="red" size="+2">[% error %]</font></p>
			[% END %]
			[ <a href="?page=people&amp;home=1">Home</a> ] |
			[% IF people %]
				[ <a href="?page=censuses">Census Index</a> ] |
			[% END %]
			[ <a href="?page=surnames">Surname Index</a> ] |
			[ <a href="?page=history">Family History</a> ] |
			[ <a href="?page=todo">To Do</a> ] |
			[ <a href="?page=calendar">Calendar</a> ]
		</center>
		[% IF censuses %]
			<br>
			<ul>
				[% FOREACH census IN censuses %]
					<li>
						<a href="?page=censuses&amp;census=[%- census | uri -%]">[% census %]</a>
					</li>
				[% END %]
			</ul>
		[% ELSIF people %]
			<br>
			<ul>
				[% FOREACH person IN people %]
					<li>
						<a href="?page=people&amp;entry=[%- person.entry %]">[% person.title %]</a>
					</li>
				[% END %]
			</ul>
		[% END %]
		<p>
			<i>
			Generated by <a href="https://github.com/nigelhorne/ged2site">Ged2Site</a>.
			[% IF updated %]
				[% USE date %]
				Last updated on [% date.format(time = updated, format = '%d %b %Y', locale = lingua.language_code_alpha2) %]
			[% END %]
			</i>
		</p>
	</body>
</html>
